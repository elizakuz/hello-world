#include <stdio.h>
#include <string.h>

int main(){
	char s[100];
	scanf("%s",s);
	int n=strlen(s);
	int pal = 1;
	for (char *p =s,*q=s+n-1; p<q; ++p, --q){
		if (*p==0x20) ++q;
			else if (*q==020) --p;
				else if (*p != *q){
					pal=0;
					break;
				}
	}
	if (pal==1)
		printf("yes\n");
	else
		printf("no\n");
}
